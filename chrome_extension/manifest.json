{
  "name": "Euler captcha",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Extension that solves project euler captions",
  "homepage_url": "http://extensionizr.com",
  "permissions":[
      "https://projecteuler.net/*"
  ],
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "content_scripts": [
    {
      "matches": [
        "https://www.google.com/*"
      ],
      "js": [
        "src/inject/inject.js"
      ]
    }
  ]
}
